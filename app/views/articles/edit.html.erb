<h1 class = "heading"> Here you can edit your post </h1>
<%= form_for :article, url: article_path(@article), method: :patch do |f| %>

<%= f.label :title %>
<br>
<%= f.text_field :title %>
<br>

<%= f.label :body %>
<br>
<%= f.text_area :body %>
<br>

<%= f.submit "submit", class: "btn btn-primary"%>

<% end %>